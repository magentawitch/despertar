script/source = "extends Escena


func _on_PuertaPasillo_pressed():
	
	var d2 = randi() % 2
	match d2:
		0:
			dice(\"Jade\", \"Tengo que completar este estúpido examen.\")
		1:
			dice(\"Profesore\", \"Dónde crees que vas, Jade? Completa tu examen como el resto de la clase.\")



func _on_Examen_pressed():
	anotar_en_el_diario(\"\"\"
	Llegué unos minutos antes, eso es ser una persona eficiente. 
	Podría haber repasado antes del examen, pero quién no preferiria escuchar a una de las mejores divas pop de antes.
	Seguro si apruebo es gracias a Miss Clarkson.
	\"\"\")
	cambiar_escena_a('03_examen')

var clicks_reloj = 0
func _on_Reloj_pressed():
	clicks_reloj += 1
	if clicks_reloj == 1:
		dice(\"Jade\", \"El reloj no funciona desde que estudio acá. Para ser una sociedad que está tan atada a la tecnología, a veces dejan mucho que desear\")
	if clicks_reloj >= 2:
		dice(\"Jade\", \"Claro que si todes tienen un implante que les indica siempre la hora, no necesitan este viejo reloj de pared...\")
		dice(\"Jade\", \"Se siente como un recordatorio para les que no tenemos implantes. Quieren que seamos conscientes de que esta sociedad no está pensada para nosotres\")


func _on_Profesore_pressed():
	dice(\"Profesore\", \"El examen ya fue repartido. Llegaste con el tiempo justo.\")
	dice(\"Profesore\", \"Recuerda, llegar temprano es estar a tiempo. Llegar a tiempo es llegar tarde, y llegar tarde es inaceptable\")
"

