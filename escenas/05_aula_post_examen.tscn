script/source = "extends Escena

func _ready():
	registrar_hito(\"paso_por_el_agujero\")
	dice(\"Jade\", \"Uf, atravesar eso fue dificil\")
	dice(\"Jade\", \"Oh, una cámara!\")
	agarrar_camara()
	dice(\"Jade\", \"Ahora no me queda otra que sacarle foto a alguno de estos pupitres\")
	dice(\"Jade\", \"La pregunta es... cual?\")

func _on_PuertaPasillo_pressed():
	dice(\"Jade\", \"Debo satisfacer mis ganas de fotografear antes\")

func _on_PuertaArmario_pressed() -> void:
	if no_ocurrio_que(\"vio_puerta_armario\"):
		dice(\"Jade\", \"Es el perturbador armario del aula\")
		registrar_hito(\"vio_puerta_armario\")
		
	if no_saco_foto(\"mesa_profesore\"):
		dice(\"Jade\", \"No puedo entrar sin antes haber sacado una foto\")
		return
	
	dice(\"Jade\", \"Entremos\")
	dice(\"Profesore\", \"Espera jade!\")
	dice(\"Jade\", \"No Profesore! No podrá detenerme!\")
	cambiar_escena_a(\"04_pasillo_escuela2\")
	dice(\"Jade\", \"Wow\")
	dice(\"Jade\", \"It's bigger on the inside!\")


var ya_hablo_con_profesore = false

func _on_Profesore_pressed() -> void:
	if ya_hablo_con_profesore:
		dice(\"Profesore\", \"No entiendo que estás haciendo\")
	else:
		ya_hablo_con_profesore = true
		dice(\"Profesore\", \"Jade, a donde vas?\")


func _on_Banco1_pressed() -> void:
	if ya_saco_foto(\"mesa_profesore\"):
		dice(\"Jade\", \"Ya saqué una foto, no quiero todas\")
		return
	dice(\"Jade\", \"Este pupitre está muy cerca de la puerta\")
	
	if vio_el_pupitre and not reconcidero_el_pupitre:
		dice(\"Jade\", \"Me gusta mas el que está al lado de la pizarra-monitor\")
		reconcidero_el_pupitre = true


func _on_Banco2_pressed() -> void:
	if ya_saco_foto(\"mesa_profesore\"):
		dice(\"Jade\", \"Ya saqué una foto, no quiero todas\")
		return
	dice(\"Jade\", \"Este pupitre está muy al frente\")
	
	if vio_el_pupitre and not reconcidero_el_pupitre:
		dice(\"Jade\", \"Me gusta mas el que está al lado de la pizarra-monitor\")
		reconcidero_el_pupitre = true

var vio_el_pupitre = false
var reconcidero_el_pupitre = false
func _on_Banco3_pressed() -> void:
	vio_el_pupitre = true
	if ya_saco_foto(\"mesa_profesore\"):
		dice(\"Jade\", \"Ya le saqué una foto\")
		match epigrafe_elegido_para(\"mesa_profesore\"):
			\"oda_uniformidad\":
				dice(\"Jade\", \"No quiero otra foto de una mesa aburrida\")
			\"futuro_anicos\":
				dice(\"Jade\", \"No quiero otra foto que me traiga a este presente\")
			_:
				dice(\"Jade\", \"No quiero otra\")
		return
		
	if not reconcidero_el_pupitre:
		dice(\"Jade\", \"Este me gusta pero, no estoy segure\")
	elif no_saco_foto(\"mesa_profesore\"):
		dice(\"Jade\", \"Serás añadido a la colección de mi diario!\")
		agrega_foto_al_diario(\"mesa_profesore\")


func _on_Banco4_pressed() -> void:
	if ya_saco_foto(\"mesa_profesore\"):
		dice(\"Jade\", \"Ya saqué una foto\")
		return
	if reconcidero_el_pupitre:
		dice(\"Jade\", \"Me parece que el que está a la izquierda de le profesore es el que va\")
		return
	dice(\"Jade\", \"Hmmm, este está sucio\")
	
	if vio_el_pupitre and not reconcidero_el_pupitre:
		dice(\"Jade\", \"Para eso prefiero el otro\")
		reconcidero_el_pupitre = true


func _on_Banco5_pressed() -> void:
	if ya_saco_foto(\"mesa_profesore\"):
		dice(\"Jade\", \"Ya saqué una foto, no quiero todas\")
		return
	if reconcidero_el_pupitre:
		dice(\"Jade\", \"Me parece que el que está a la izquierda de le profesore es el que va\")
		return
		
	dice(\"Jade\", \"Hmmm, este está escrito y dibujado con laser\")
	
	if vio_el_pupitre and not reconcidero_el_pupitre:
		dice(\"Jade\", \"Prefiero el otro\")
		reconcidero_el_pupitre = true
"

